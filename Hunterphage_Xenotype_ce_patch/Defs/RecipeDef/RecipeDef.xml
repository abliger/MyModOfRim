<Defs>
    <RecipeDef ParentName="AmmoRecipeBase">
        <defName>MakeBulletBlunderbussCE_Ammo</defName>
        <label>make flintlock pellet x200</label>
        <description>Craft 200 flintlock pellet.</description>
        <jobString>Making flintlock pellet.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>100</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Steel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <BulletBlunderbussCE_Ammo>200</BulletBlunderbussCE_Ammo>
        </products>
        <recipeUsers Inherit="False">
            <li>HunterWorkbench</li>
        </recipeUsers>
        <workAmount>280</workAmount>
        <researchPrerequisite>FlintlockArmaments</researchPrerequisite>
    </RecipeDef>

