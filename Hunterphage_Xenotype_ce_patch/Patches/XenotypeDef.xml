<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Hunterphage Xenotype</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/XenotypeDef[defName="Hunterphage"]</xpath>
                    <value>
                        <nameMaker>PawnNamerGenHunterphage</nameMaker>
                        <chanceToUseNameMaker>1</chanceToUseNameMaker>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>